{% set typekitCode = configResolver.getParameter( 'SiteSettings.TypeKitCode', 'ngmore' ) %}

{% if typekitCode is not empty %}
    <script type="text/javascript" src="//use.typekit.com/{{ typekitCode }}.js"></script>
    <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
{% endif %}
