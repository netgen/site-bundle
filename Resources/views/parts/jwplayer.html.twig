<script src="{{ asset('/bundles/netgenmoredemo/js/jwplayer/jwplayer.js') }}"></script>

{% if ezpublish.configResolver.hasParameter( 'site_settings.jwplayer_key', 'ngmore' ) %}
    {% set JWPlayerKey = ezpublish.configResolver.getParameter( 'site_settings.jwplayer_key', 'ngmore' ) %}
    {% if JWPlayerKey is not empty %}
        <script type="text/javascript">jwplayer.key="{{ JWPlayerKey|raw|escape( 'js' ) }}";</script>
    {% endif %}
{% endif %}



