{% if ezpublish.configResolver.hasParameter( 'site_settings.sublime_video_code', 'ngmore' ) %}
    {% set sublimeVideoCode = ezpublish.configResolver.getParameter( 'site_settings.sublime_video_code', 'ngmore' ) %}

    {% if sublimeVideoCode is not empty %}
        <script type="text/javascript" src="//cdn.sublimevideo.net/js/{{ sublimeVideoCode|raw|escape( 'js' ) }}.js"></script>
    {% endif %}
{% endif %}
