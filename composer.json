{
    "name": "netgen/site-bundle",
    "description": "Netgen Site Bundle is an eZ Platform bundle that implements common features that are used by Netgen to implement websites",
    "license": "GPL-2.0-only",
    "type": "ezplatform-bundle",
    "keywords": [
        "ezpublish",
        "ezplatform",
        "site-bundle",
        "netgen"
    ],
    "homepage": "https://github.com/netgen/site-bundle",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "https://netgen.io"
        }
    ],
    "require": {
        "ezsystems/ezpublish-kernel": "^7.5",
        "netgen/ezplatform-site-api": "^2.6.1 || ^3.0",
        "netgen/ez-forms-bundle": "^2.0.2",
        "netgen/siteaccess-routes-bundle": "^1.1",
        "netgen/metadata-bundle": "^2.0",
        "netgen/content-type-list-bundle": "^1.3",
        "netgen/enhanced-selection-bundle": "^3.1",
        "netgen/birthday-bundle": "^1.3",
        "netgen/open-graph-bundle": "^1.3",
        "netgen/tagsbundle": "^3.4",
        "twig/twig": "^2.11.3",
        "twig/extensions": "^1.5.4",
        "doctrine/common": "^2.10",
        "doctrine/orm": "^2.6.3",
        "knplabs/knp-menu-bundle": "^2.2.1 || ^3.0",
        "sensio/framework-extra-bundle": "^5.3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.8",
        "friendsofphp/php-cs-fixer": "^2.14.2"
    },
    "conflict": {
        "netgen/block-manager": "*",
        "netgen/layouts-core": "<1.0"
    },
    "autoload": {
        "psr-4": {
            "Netgen\\Bundle\\SiteBundle\\": "bundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netgen\\Bundle\\SiteBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always",
        "coverage": "@php -dzend_extension=xdebug.so vendor/bin/phpunit --colors=always",
        "fix": "@php vendor/bin/php-cs-fixer fix"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    }
}
