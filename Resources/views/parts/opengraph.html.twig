{% if openGraph is defined %}
    {% for key, value in openGraph %}
        <meta property="{{ key|trim }}" content="{{ value|trim }}" />
    {% endfor %}
{% endif %}

{% if content is defined %}
    {% set metaTags = get_netgen_open_graph( content ) %}

    {% for metaTag in metaTags %}
        {% if openGraph is not defined or openGraph[metaTag.tagName] is not defined %}
            <meta property="{{ metaTag.tagName|trim }}" content="{{ metaTag.tagValue|trim }}" />
        {% endif %}
    {% endfor %}
{% endif %}
