{% set main_pagelayout = viewbaseLayout|default('@NetgenSite/pagelayout_empty.html.twig') %}

{% if noLayout is not defined or not noLayout %}
    {% if ezpublish.configResolver.hasParameter('pagelayout', 'ngsite') %}
        {% set main_pagelayout = ezpublish.configResolver.getParameter('pagelayout', 'ngsite') %}
    {% elseif pagelayout is defined %}
        {% set main_pagelayout = pagelayout %}
    {% elseif ezpublish.configResolver.hasParameter('pagelayout') %}
        {% set main_pagelayout = ezpublish.configResolver.getParameter('pagelayout') %}
    {% endif %}
{% endif %}

{% extends main_pagelayout %}
