{% set typekit_code = config_resolver.getParameter( 'SiteSettings.TypeKitCode', 'ngmore' ) %}

{% if typekit_code is not empty %}
    <script type="text/javascript" src="//use.typekit.com/{{ typekit_code }}.js"></script>
    <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
{% endif %}
